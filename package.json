{
  "name": "fullstack_boilerplate_mrfn",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "server-start": "npm start --prefix public",
    "server-dev": "npm dev --prefix public",
    "server-test": "npm test --prefix public",
    "client": "npm dev --prefix frontend",
    "dev": "concurrently \"npm run server-dev\" \"npm run client\"",
    "build": "npm build --prefix frontend",
    "setup": "npm install && npm install --prefix public && npm install --prefix frontend"
  },
  "author": "Rinaldo Benaccetta",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "concurrently": "^7.3.0"
  }
}
